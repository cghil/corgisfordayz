<div id="wrapper">
  <div id="sign-up">
    <h2>Sign Up</h2>
    <form action="/users" method="post">
      <p><label id="email">Email: <input type="email" name="user[email]"></label></p>
      <p><label>Username: <input type="text" name="user[username]"></label></p>
      <p><label>Password: <input type="password" name="user[password]"></label></p>
      <p><input type="submit" value="Sign-Up"></p>
    </form>
  </div>
  <div id="sign-in">
    <h2>Sign In</h2>
    <form action="/session" method="post">
      <p><label>Username: <input type="text" name="user[username]"></label></p>
      <p><label>Password: <input type="password" name="user[password]"></label></p>
      <p><input type="submit" value="Sign-In"></p>
    </form>
  </div>
  <link rel="stylesheet" href="/css/sign_in.css">
</div>
